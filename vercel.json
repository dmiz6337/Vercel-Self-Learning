{
  "version": 2,
  "env": {
    "VERCEL_ENV": "production",
    "VERCEL_URL": "strata-management-vercel.vercel.app",
    "VERCEL_GIT_PROVIDER": "github",
    "VERCEL_GIT_BRANCH": "main",
    "VERCEL_GIT_COMMIT_AUTHOR_LOGIN": "dmiz6337",
    "VERCEL": "1"
  },
  "rewrites": [
    {
      "source": "/phpinfo",
      "destination": "/api/index.php"
    }
  ],
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3",
      "excludeFiles": "**/*.{next,ts,tsx,json,js,lock,yml,yaml,md,mdx,log,env,zip,cache}"
    }
  }
}