{
  "version": 2,
  "env": {
    "VERCEL_ENV": "production",
    "VERCEL_URL": "strata-management-vercel.vercel.app",
    "VERCEL_GIT_PROVIDER": "github",
    "VERCEL_GIT_BRANCH": "main",
    "VERCEL_GIT_COMMIT_AUTHOR_LOGIN": "dmiz6337",
    "VERCEL": "1"
  },
  "routes": [
    { "src": "/phpinfo", "dest": "/api/index.php" },
    { "src": "/api/auth/$1", "dest": "app/api/auth/$1" }
  ],
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3",
      "excludeFiles": "{public/**,uploads/**,vendor/**,node_modules/**,*.cache,*.zip,*.json,.next/**}"
    }
  }
}