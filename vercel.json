{
    "version": 2,
    "env": {
      "VERCEL_ENV": "development",
      "VERCEL_URL": "strata-body-corporate.vercel.app",
      "VERCEL_GIT_PROVIDER": "github",
      "VERCEL_GIT_BRANCH": "main",
      "VERCEL_GIT_COMMIT_AUTHOR_LOGIN": "dmiz6337",
      "VERCEL": "1"
    },
    "buildCommand": "npm run custom-build",
    "crons": [
      {
        "path": "/api/cron/daily-payment",
        "schedule": "0 0 * * *"
      }
    ],
    "builds": [
      { "src": "api/**/*.php", "use": "@vercel/php@0.7.3" },
      { "src": "package.json", "use": "@vercel/next" }
    ],
    "routes": [
      { "src": "/api/(.*\\.php)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/$1" }
    ]
}